<link rel='stylesheet' type='text/css' href='../index.css'>
<div class='main'>
  <h1 class='title'><%=@category%></h1>
  <ul>
  	<%@contacts.each do |contact|%>
  	  <div class='item'>
  	    <li class='text'>
  	    	<a href='/<%=@category%>/<%=contact%>'>
  	    		<%=contact%>
  	    	</a>
  	    </li>
  	  </div>
  	<% end %>
  </ul>
</div>

<form action='/' method='get'>
	<button style='width: 10em' type='submit'>Go Back</button>
</form>